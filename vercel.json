{
  "rewrites": [
    { "source": "^/$", "destination": "/DashboardMain" },
    { "source": "^/Car$", "destination": "/CarMain" },
    { "source": "^/Car/Add$", "destination": "/CarAdd" },
    { "source": "^/Car/Detail/([^/]+)$", "destination": "/CarEdit" },

    { "source": "^/Book$", "destination": "/BookMain" },
    { "source": "^/Book/Add/ChooseCar$", "destination": "/BookChooseCar" },
    { "source": "^/Book/Add/([^/]+)$", "destination": "/BookAdd" },
    { "source": "^/Book/Detail/([^/]+)/([^/]+)$", "destination": "/BookDetail" },
    { "source": "^/Book/Create/([^/]+)$", "destination": "/BookCreateRent" },

    { "source": "^/Rent$", "destination": "/RentMain" },
    { "source": "^/Rent/Add/([^/]+)$", "destination": "/RentAdd" },
    { "source": "^/Rent/Detail/([^/]+)$", "destination": "/RentDetail" },
    { "source": "^/Rent/Checkout/([^/]+)$", "destination": "/RentCheckout" },
  ]
}